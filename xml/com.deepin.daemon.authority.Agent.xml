<interface name="com.deepin.daemon.Authority.Agent">
  <method name="RequestEchoOff">
    <arg name="message" type="s" direction="in"/>
    <arg name="message" type="s" direction="out"/>
  </method>
  <method name="RequestEchoOn">
    <arg name="message" type="s" direction="in"/>
    <arg name="message" type="s" direction="out"/>
  </method>
  <method name="DisplayErrorMsg">
    <arg name="errtype" type="s" direction="in"/>
    <arg name="message" type="s" direction="in"/>
  </method>
  <method name="DisplayTextInfo">
    <arg name="message" type="s" direction="in"/>
  </method>
  <method name="RespondResult">
    <arg name="authType" type="s" direction="in"/>
  </method>
</interface>
